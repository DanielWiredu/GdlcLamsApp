@page "/pages-empty"
@rendermode InteractiveServer
@inject IJSRuntime JsRuntime;


<PageBreadcrumb Subtitle="Pages" Title="Starter"/>

<MudButton Color="Color.Success" Variant="Variant.Filled" Href="/reports/workerlist" Target="_blank"> Test Crystal Report </MudButton>

@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JsRuntime.InvokeVoidAsync("loadConfig");
            await JsRuntime.InvokeVoidAsync("loadApps");
        }
    }
}
